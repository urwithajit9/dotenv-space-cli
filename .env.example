# ── Application ──
DEBUG=True
  # (required)
DJANGO_SETTINGS_MODULE=myproject.settings
  # (required)


# ── Security ──
# Comma-separated list of allowed host/domain names
ALLOWED_HOSTS=localhost,127.0.0.1
  # (required)
# Django secret key (generate with: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())')
SECRET_KEY=your_secret_key_value
  # (required)


# ── Database ──
DATABASE_URL=your_database_url_value
  # (required)
MONGODB_DB_NAME=your_mongodb_db_name_value
MONGODB_URI=mongodb://localhost:27017


# ── Cloud Provider ──
AWS_ACCOUNT_ID=your_aws_account_id_value
AWS_PROFILE=your_aws_profile_value
AWS_REGION=us-east-1


# ── Logging ──
LOGTAIL_SOURCE_TOKEN=your_logtail_source_token_value


# ── Payments ──
# Publishable key (safe for client-side)
STRIPE_PUBLIC_KEY=your_stripe_public_key_value
# Secret key (server-side only)
STRIPE_SECRET_KEY=your_stripe_secret_key_value
# Secret for verifying webhook signatures
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_value

